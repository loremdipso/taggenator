{
	"extensions": [".txt"],

	"synonyms": {
		"pre": "prefix",
		"post": "postfix"
	},

	"prefixes": [
		"getmore:"
	],

	"commands": {
		"ls": "ls"
	},

	"openerconfig": {
		"default": {
			"Open": "gnome-open \"%s\""
		},

		"video": {
			"Open": "vlc \"%s\"",
			"Update": "vlc \"%s\"",
			"Close": "killall vlc"
		},

		"comic": {
			"Open": "mcomix \"%s\"",
			"Close": "killall mcomix"
		}
	}
}
